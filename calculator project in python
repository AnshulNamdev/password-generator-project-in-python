First = input("enter first number:")
Operator = input("enter operator(+,-,*,/,%):")
Second = input("enter second number:")
First = int(first)
Second = int(second)
if operator == "+":
     print(first + second)
elif operator == "-":
       print(first - second)
elif operator == "*":
       print(first * second)
elif operator == "/":
       print(first / second)
elif operator == "%":
       print(first % second)
else: 
       print("invalid operator")
